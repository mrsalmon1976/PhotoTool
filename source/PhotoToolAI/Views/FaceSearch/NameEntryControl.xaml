<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PhotoToolAI.Views.FaceSearch.NameEntryControl">
    
    <Frame x:Name="frame" Padding="0" Margin="3" >
        <HorizontalStackLayout Padding="0" Margin="0,0,0,0">
            <Image
                    x:Name="imgFace"
                    Grid.Column="0"
                    Aspect="AspectFit"
                    MaximumWidthRequest="75"
                    MaximumHeightRequest="75"
                    WidthRequest="75"
                
                    />
            <Entry x:Name="entry" WidthRequest="150" MaximumWidthRequest="150" VerticalOptions="Center" />
    </HorizontalStackLayout>
    </Frame>
</ContentView>
