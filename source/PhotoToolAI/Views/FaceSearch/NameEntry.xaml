<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PhotoToolAI.Views.FaceSearch.NameEntry">
        <HorizontalStackLayout Padding="0" Margin="0,0,0,3">
            <RadioButton GroupName="NameEntryButtonGroup" Padding="0" />
            <Border x:Name="border" StrokeThickness="2" Padding="0" >
                <Entry x:Name="entry" WidthRequest="150" MaximumWidthRequest="150" />
            </Border>
        </HorizontalStackLayout>
</ContentView>
