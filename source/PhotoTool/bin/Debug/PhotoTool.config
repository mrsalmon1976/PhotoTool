<phototool>
  <settings>
    <main_form>
      <height>555</height>
      <left>100</left>
      <top>100</top>
      <width>1142</width>
    </main_form>
    <image_options>
      <image_length>1024</image_length>
      <thumbnail_length>100</thumbnail_length>
      <generate_thumbs>1</generate_thumbs>
      <replace_spaces>1</replace_spaces>
      <image_quality>90</image_quality>
      <use_imagemagick>0</use_imagemagick>
    </image_options>
    <folders>
      <add_file_folder>C:\Temp\in</add_file_folder>
      <add_dir_folder>C:\Temp\in</add_dir_folder>
      <output_folder>C:\Temp\out</output_folder>
      <project_folder>
      </project_folder>
    </folders>
    <thumbnails>
      <name_format>tn_{FILE_NAME}{EXTENSION}</name_format>
    </thumbnails>
  </settings>
</phototool>